<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="42.0" prefWidth="950.0" type="HBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="participantName" prefHeight="45.0" prefWidth="576.0" text="Title" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Label fx:id="participantAge" prefHeight="45.0" prefWidth="210.0" text="Age" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Button fx:id="btnActivateParticipant" mnemonicParsing="false" onAction="#activateParticipant" prefHeight="44.0" prefWidth="141.0" text="Start Evaluation" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
