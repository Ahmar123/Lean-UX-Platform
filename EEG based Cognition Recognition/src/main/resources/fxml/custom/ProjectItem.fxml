<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="65.0" prefWidth="855.0" spacing="10.0" type="HBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="projectTitle" prefHeight="45.0" prefWidth="642.0" text="Title" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Button fx:id="btnEvaluate" mnemonicParsing="false" onAction="#handleProjectEvent" prefHeight="39.0" prefWidth="183.0" text="Evaluate Project" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
